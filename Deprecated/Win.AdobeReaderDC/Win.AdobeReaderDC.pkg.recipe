<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
	<string>Downloads Adobe Reader DC and creates pkg.</string>
	<key>Identifier</key>
	<string>com.github.jeremylarose.pkg.Win.AdobeReaderDC</string>
	<key>Input</key>
	<dict>
		<key>NAME</key>
		<string>AdobeReaderDC</string>
	</dict>
	<key>MinimumVersion</key>
	<string>0.6.1</string>
	<key>ParentRecipe</key>
	<string>com.github.jeremylarose.download.Win.AdobeReaderDC</string>
	<key>Process</key>
	<array>
		<dict>
		  <key>Processor</key>
		  <string>com.github.jeremylarose.SharedProcessors/PkgCreator7z</string>
			<key>Arguments</key>
			<dict>
				<key>install_flags</key>
				<string>/sAll /rs /msi EULA_ACCEPT=YES</string>
				<key>pkg_name</key>
				<string>%NAME%-%major_version%.%mid_version%.%minor_version%.exe</string>
			</dict>
    </dict>
	</array>
</dict>
</plist>
