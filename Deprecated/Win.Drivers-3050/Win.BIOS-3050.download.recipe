<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
	<string>Downloads the latest 3050 BIOS Update.</string>
	<key>Identifier</key>
	<string>com.github.jeremylarose.download.Win.BIOS-3050</string>
	<key>Input</key>
	<dict>
		<key>NAME</key>
		<string>3050</string>
	</dict>
	<key>MinimumVersion</key>
	<string>0.6.1</string>
	<key>Process</key>
	<array>
		<dict>
				<key>Processor</key>
				<string>URLTextSearcher</string>
				<key>Arguments</key>
				<dict>
						<key>url</key>
						<string>http://www.dell.com/support/home/us/en/19/product-support/product/optiplex-3050-desktop/drivers</string>
						<key>re_pattern</key>
            <string>downloads.dell.com[\W](?P&lt;folder&gt;[\S]+)[\W](?P&lt;number&gt;[\S]+)[\W]OptiPlex_3050_(?P&lt;version&gt;[\d\.]+).exe</string>
				</dict>
		</dict>
		<dict>
			<key>Arguments</key>
			<dict>
				<key>url</key>
				<string>https://downloads.dell.com/%folder%/%number%/OptiPlex_3050_%version%.exe</string>
			</dict>
			<key>Processor</key>
			<string>URLDownloader</string>
		</dict>
	</array>
</dict>
</plist>
