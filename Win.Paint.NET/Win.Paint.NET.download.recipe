<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
	<string>Downloads and extracts the latest version of Paint.NET.</string>
	<key>Identifier</key>
	<string>com.github.jeremylarose.download.Win.Paint.NET</string>
	<key>Input</key>
	<dict>
		<key>NAME</key>
		<string>Paint.NET</string>
	</dict>
	<key>MinimumVersion</key>
	<string>0.6.1</string>
	<key>Process</key>
	<array>
		<dict>
				<key>Processor</key>
				<string>URLTextSearcher</string>
				<key>Arguments</key>
				<dict>
						<key>url</key>
						<string>http://www.dotpdn.com/downloads/pdn.html</string>
						<key>re_pattern</key>
            <string>http://www.dotpdn.com/files/paint.net.(?P&lt;version&gt;[\d\.]+)install.zip</string>
				</dict>
		</dict>
		<dict>
			<key>Arguments</key>
			<dict>
				<key>url</key>
				<string>http://www.dotpdn.com/files/paint.net.%version%install.zip</string>
			</dict>
			<key>Processor</key>
			<string>URLDownloader</string>
		</dict>
		<dict>
			<key>Processor</key>
			<string>Unarchiver</string>
		</dict>
		<dict>
				<key>Processor</key>
				<string>EndOfCheckPhase</string>
		</dict>
	</array>
</dict>
</plist>
