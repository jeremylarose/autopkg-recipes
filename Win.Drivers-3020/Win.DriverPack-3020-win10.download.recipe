<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
	<string>Downloads the latest 3020 drivers cab.</string>
	<key>Identifier</key>
	<string>com.github.jeremylarose.download.Win.DriverPack-3020-win10</string>
	<key>Input</key>
	<dict>
		<key>NAME</key>
		<string>3020</string>
		<key>USER_AGENT</key>
		<string>Mozilla/5.0 (Windows NT 6.1; WOW64; Trident/7.0; AS; rv:11.0) like Gecko</string>
	</dict>
	<key>MinimumVersion</key>
	<string>0.6.1</string>
	<key>Process</key>
	<array>
		<dict>
				<key>Processor</key>
				<string>URLTextSearcher</string>
				<key>Arguments</key>
				<dict>
						<key>url</key>
						<string>https://www.dell.com/support/article/us/en/19/how13362/optiplex-3020-windows-10-driver-pack</string>
						<key>request_headers</key>
						<dict>
										<key>user-agent</key>
										<string>%USER_AGENT%</string>
						</dict>
						<key>re_pattern</key>
            <string>downloads.dell.com/(?P&lt;urlstring&gt;[\S]+)-(?P&lt;platform&gt;[\S]+)-(?P&lt;version&gt;[\S]+)-(?P&lt;id&gt;[\S]+).CAB</string>
						<key>request_headers</key>
						<dict>
										<key>user-agent</key>
										<string>%USER_AGENT%</string>
						</dict>
				</dict>
		</dict>
		<dict>
			<key>Arguments</key>
			<dict>
				<key>url</key>
				<string>http://downloads.dell.com/%urlstring%-%platform%-%version%-%id%.CAB</string>
			</dict>
			<key>Processor</key>
			<string>URLDownloader</string>
		</dict>
	</array>
</dict>
</plist>
