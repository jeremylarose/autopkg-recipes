<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
	<string>Downloads the latest version of GoogleChrome x86 and x64 Executables.</string>
	<key>Identifier</key>
	<string>com.github.jeremylarose.download.Win.GoogleChrome_test</string>
	<key>Input</key>
	<dict>
		<key>NAME</key>
		<string>GoogleChrome</string>
	</dict>
	<key>MinimumVersion</key>
	<string>0.6.1</string>
	<key>Process</key>
	<array>
		<dict>
			<key>Arguments</key>
			<dict>
				<key>filename</key>
				<string>%NAME%-x64.msi</string>
				<key>url</key>
				<string>https://dl.google.com/dl/chrome/install/googlechromestandaloneenterprise64.msi</string>
			</dict>
			<key>Processor</key>
			<string>URLDownloader</string>
		</dict>
		<dict>
			<key>Arguments</key>
			<dict>
				<key>filename</key>
				<string>%NAME%-x86.msi</string>
				<key>url</key>
				<string>https://dl.google.com/dl/chrome/install/googlechromestandaloneenterprise.msi</string>
			</dict>
			<key>Processor</key>
			<string>URLDownloader</string>
		</dict>
		<dict>
		  <key>Processor</key>
		  <string>com.github.jeremylarose.SharedProcessors/Unarchiver7z</string>
			<key>Arguments</key>
			<dict>
	  		<key>extract_path</key>
		  	<string>%RECIPE_CACHE_DIR%/InstallerExtract</string>
			</dict>
    </dict>
		<dict>
				<key>Processor</key>
				<string>com.github.hansen-m.SharedProcessors/BESRelevanceProvider</string>
				<key>Arguments</key>
				<dict>
						<key>bes_filepath</key>
						<string>%pathname%</string>
						<key>output_var_name</key>
						<string>version</string>
						<key>bes_relevance</key>
						<string>preceding text of first " Copyright 2011 Google Inc." of substrings separated by "%00" whose (it ends with "Google Inc.") of lines whose (it contains "Installer") of file "%RECIPE_CACHE_DIR%/InstallerExtract/[5]SummaryInformation" as version</string>
				</dict>
		</dict>
		<dict>
				<key>Processor</key>
				<string>EndOfCheckPhase</string>
		</dict>
	</array>
</dict>
</plist>
