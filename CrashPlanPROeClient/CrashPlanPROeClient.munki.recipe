<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Comment</key>
	<string>Downloads the latest version of CrashPlanPROeClient and imports it into Munki</string>
	<key>Identifier</key>
	<string>com.github.jeremylarose.munki.CrashPlanPROeClient</string>
	<key>Input</key>
	<dict>
		<key>MUNKI_REPO_SUBDIR</key>
		<string>apps/%NAME%</string>
		<key>NAME</key>
		<string>CrashPlanPROeClient</string>
		<key>pkginfo</key>
		<dict>
			<key>catalogs</key>
			<array>
				<string>testing</string>
			</array>
			<key>description</key>
			<string>Client for CrashPlan backup software.</string>
			<key>category</key>
			<string>Backup</string>
			<key>developer</key>
			<string>Code42</string>
			<key>display_name</key>
			<string>CrashPlan PROe</string>
			<key>name</key>
			<string>%NAME%</string>
			<key>version</key>
			<string>%version%</string>
			<key>unattended_instal</key>
			<true/>
		</dict>
	</dict>
	<key>MinimumVersion</key>
	<string>0.6.1</string>
	<key>ParentRecipe</key>
	<string>com.github.homebysix.pkg.CrashPlanPROeClient</string>
    <key>Process</key>
    <array>
        <dict>
            <key>Processor</key>
            <string>MunkiImporter</string>
            <key>Arguments</key>
            <dict>
							  <key>pkg_path</key>
							  <string>%RECIPE_CACHE_DIR%/CrashPlanPROe-%version%.pkg</string>
                <key>repo_subdirectory</key>
                <string>%MUNKI_REPO_SUBDIR%</string>
            </dict>
        </dict>
    </array>
</dict>
</plist>
